{
  "manifest_version": 3,
  "name": "Screenshot Extension",
  "version": "1.0",
  "permissions": [
    "tabs",
    "downloads",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}